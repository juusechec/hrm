{% extends 'hrm.html.twig' %}

{% block title %}Tipo de Examen Medico{% endblock %}

{% block body %}
    <h1>Tipo de Examen Medico</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipo_examen_medico.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ tipo_examen_medico.nombre }}</td>
            </tr>
            <tr>
                <th>Descripción</th>
                <td>{{ tipo_examen_medico.descripcion }}</td>
            </tr>
            <tr>
                <th>Abreviación</th>
                <td>{{ tipo_examen_medico.abreviacion }}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ tipo_examen_medico.orden }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ tipo_examen_medico.activo ? 'Sí' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('tipo_examen_medico_index') }}">Volver a la lista</a>

    <a href="{{ path('tipo_examen_medico_edit', {'id': tipo_examen_medico.id}) }}">Editar</a>

    {{ include('tipo_examen_medico/_delete_form.html.twig') }}
{% endblock %}
