<form method="post" action="{{ path('otrosi_delete', {'id': otrosi.id}) }}" onsubmit="return confirm('¿Está seguro que desea eliminar este ítem?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ otrosi.id) }}">
    <button class="btn">Delete</button>
</form>
