{% extends 'hrm.html.twig' %}

{% block title %}ConceptoOtrosi{% endblock %}

{% block body %}
    <h1>ConceptoOtrosi</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ concepto_otrosi.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ concepto_otrosi.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ concepto_otrosi.descripcion }}</td>
            </tr>
            <tr>
                <th>Abreviacion</th>
                <td>{{ concepto_otrosi.abreviacion }}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ concepto_otrosi.orden }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ concepto_otrosi.activo ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('concepto_otrosi_index') }}">Volver a la Lista</a>

    <a href="{{ path('concepto_otrosi_edit', {'id': concepto_otrosi.id}) }}">edit</a>

    {{ include('concepto_otrosi/_delete_form.html.twig') }}
{% endblock %}
