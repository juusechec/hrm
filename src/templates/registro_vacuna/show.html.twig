{% extends 'hrm.html.twig' %}

{% block title %}RegistroVacuna{% endblock %}

{% block body %}
    <h1>RegistroVacuna</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ registro_vacuna.id }}</td>
            </tr>
            <tr>
                <th>FechaDosis</th>
                <td>{{ registro_vacuna.fechaDosis ? registro_vacuna.fechaDosis|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FechaVencimientoDosis</th>
                <td>{{ registro_vacuna.fechaVencimientoDosis }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ registro_vacuna.observacion }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('registro_vacuna_index') }}">Volver a la Lista</a>

    <a href="{{ path('registro_vacuna_edit', {'id': registro_vacuna.id}) }}">Editar</a>

    {{ include('registro_vacuna/_delete_form.html.twig') }}
{% endblock %}
